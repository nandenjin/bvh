{
  "author": "Kazumi Inada <hello@nandenjin.com> and hitsujiwool <utatanenohibi@gmail.com>",
  "name": "@nandenjin/bvh",
  "description": "BVH Parser written in JavaScript",
  "version": "0.0.0",
  "homepage": "https://github.com/nandenjin/bvh",
  "repository": {
    "type": "git",
