    "typedoc": "^0.25.13",
    "vite": "^5.2.13",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": "*"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,md,json,yaml}": "prettier --write"
